extends layout

block content
  h1 Scavanger Hunt
  include twitter.html
  div.center
    ul
      each val in found
        li
          span.found-item= val.text + "by"
          a(href='https://twitter.com/'+ val.tweet.user.screen_name target="blank")= " @"+val.tweet.user.screen_name
      each val in unfound
          li= val.text
